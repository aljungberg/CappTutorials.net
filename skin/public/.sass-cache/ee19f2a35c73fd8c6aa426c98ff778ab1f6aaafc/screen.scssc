:@template"×!// This import applies a global reset to any page that imports this stylesheet.
@import "blueprint/reset";

// To configure blueprint, edit the partials/base.sass file.
@import "partials/base";

// Import all the default blueprint modules so that we can access their mixins.
@import "blueprint";

// Combine the partials into a single screen stylesheet.
@import "partials/page";
@import "partials/form";
@import "partials/two_col";

@import "compass/css3/border-radius";
@import "compass/css3/box-shadow";
@import "compass/css3/gradient";
@import "compass/css3/text-shadow";

//$darkblue: #3041A9;
$mainblue: #3775D7;
$darkblue: #1C51A6; 
$translucentdarkblue: transparentize($darkblue, 0.05);
$lightblue: #96BDFC;
$verylightblue: #DAE7FC;

$darkbronze: #C48618;
$lightbronze: #FFD68E;

$green: #27A69D;
$lightgreen: #8FF0E9;

$shadedgray: #DEDEDE;
$lightgray: #FEFEFE;

$darkgray: #444444;

$paneltop: #FEFEFE;
$panelbottom: #E9E9E9;
$highlighttop: #F2F4F4;
$highlightbottom: #8F8B91;


body {
	font-family: "HelveticaNeue-Light", "Helvetica Neue Light",
             "Helvetica Neue", sans-serif;
	font-weight: 300;
	background: #3C3C4B;
	background-image: url("/images/dark_noise.png");
	color: $shadedgray;

	overflow: visible;
	a {
		color: $mainblue;
		text-decoration: none;

	}

	a:hover {
		color: $lightblue;
	}
}


h1 {
	font-size: 3em;
}

h2 {
	font-size: 1.5em;
	font-style: italic;
	color: $darkbronze;
}

h3 {
	font-size: 1.5em;
	margin-bottom: 0.5em;

}

#header {
	$radius: 10px;
	padding-bottom: 1em;
	padding-left: 1em;
	padding-right: 1em;
	background-image: url("/images/dark_noise.png");

}

#content{
	background-image: url("/images/dark_noise.png");
	@include single-text-shadow($darkgray, 0, 1px, 0);
	border-bottom: $darkgray 5px solid;
	padding-top: 0em;
}

.fullarticle {
	clear: both;
}

#content h2 {
	//@include border-radius(40px, 40px);
	//@include single-box-shadow(#888888, 0, 2px, 2px, 2px, inset);
	//margin-right: 20%;
	font-size: 2em;
	font-weight: bold;
	padding-top: 0.5em;
	padding-bottom: 0.5em;
	padding-left: 0.5em;
	padding-right: 1em;
	margin-left: 0em;
	margin-right: 0em;
	margin-top: 0em;
	//@include linear-gradient(color-stops(lighten($darkblue, 55%), lighten($darkblue, 50%)));
	//@include single-box-shadow(#CCCCCC, 0px, 1px, 1px, 1px);
	border-top: lighten($darkgray, 10%) 1px solid;
	border-bottom: lighten($darkgray, 10%) 0px solid;
	text-align: right;
}

.article {
	//@include linear-gradient(color-stops($paneltop, $panelbottom));
	//@include single-box-shadow(#CCCCCC, 0px, 1px, 1px, 1px);
	padding-top: 0.5em;
	padding-bottom: 1em;
	padding-left: 1em;
	padding-right: 1em;
	margin-left: 0em;
	margin-right: 0em;
	margin-bottom: 0em;
	border-top: $highlighttop 0px solid;
	border-bottom: $darkgray 0px solid;
}

#footer {
	background-color: rgba(.2, .2, .2, .2);
	padding: 1em;
	margin: 1em;
	font-size: 0.8em;
}

#accentlight {
	color: $darkbronze;
}

#accentdark {
	color: $lightbronze;
}

#accentverylight {
	color: $verylightblue;
}

#content
{
	overflow: visible;
	.fullarticle {

		overflow: visible;
		

		h1 {
			font-size: 2.2em;
			text-align: center;
			font-weight: bold;
			padding-bottom: 0.5em;
			text-align: left;
			margin-left: 0em;
		}

		h2 {

			text-align: left;
			padding-left: 0;
			margin-left: 0em;
		}

		h3 {

			color: $darkbronze;
			text-align: left;
			margin-top: 1em;
			margin-bottom: 0.5em;
			margin-left: 0em;

		}

		p {
			margin-top: 1em;
  			margin-bottom: 1em ;
  			line-height: 1.2em;
  			margin-left: 2em;
  			margin-right: 2em;
  			text-indent: 1.5em;

  		}

  		ul {
  			margin-left: 4em;
  			margin-right: 2em;
  		}
  		

  		.snippet {
  			a
  			{
  				
  			margin-left: 2em;
  			margin-right: 2em;
  			margin-bottom: 0.5em;
  			display: block;
  			}
  		}

  		code {
		  font-family: monaco, consolas, monospace;
		  font-size: 90%;
		  background-color: #002028;
		  border: 1px solid #002028;
		  @include single-text-shadow($darkgray, 0, 1px, 0);
		  @include border-radius(4px, 4px);


		}
		pre {
		  border: 1px solid #aaa;
		  background-color: #002028;
		  margin: 0 0px;
		  display: block;
		  @include border-radius(4px, 4px);
		  margin-top: 2em;
		  margin-bottom: 2em;
		  margin-left: 4em;
  			margin-right: 4em;
		}
		pre code {
		  overflow-x: auto;
		  max-height: 600px;
		  font-size: 12px;
		  line-height: 1.4em;
		  display: block;
		  padding: 10px;
		  border: 0;
		  background-color: inherit;
		}

		img {
			display: block;
			margin-left: auto;
			margin-right: auto;
			margin-top: 20px;
			margin-bottom: 20px;
			@include border-radius(4px, 4px);
			//@include single-box-shadow(#888888, 0, 1px, 2px, 2px);

		}


	}
}


hr {
	color: $darkgray;
	background-color: $darkgray;
	border-color: $darkgray;
	height: 1px;
	border: 0px;
}

/* dropdowns: general */
a.dropdown { 
	//background: #88bbd4; 
	@include linear-gradient(color-stops(lighten($darkblue,28%), lighten($darkblue,25%), lighten($darkblue, 22%)));
	//$dropshadow: single-box-shadow(#444466, 0px, 1px, 1px, 0px);
	//@include single-box-shadow($verylightblue, 0px, 0px, 4px, 2px, inset);
	//@include box-shadow($glow);
	@include single-box-shadow(#444466, 0px, 1px, 1px, 1px);
	padding: 0.25em 0.5em 0.25em 0.5em; 
	text-align: center;
	text-decoration: none; 
	font-weight: bold; 
	color: $shadedgray; 
	@include border-radius(4px, 4px);
	border-top: $lightblue 1px solid;
	//border-bottom: darken($darkblue, 10%), 1px, solid;
	//-webkit-border-radius: 4px; 
	//-moz-border-radius: 4px; 
	@include border-radius(6px, 6px); 
	@include single-text-shadow($darkgray, 0, 1px, 0);
}


#menu1 {
	position: relative;
	top: -4em;
	left: 41.1em;

	a.dropdown {		
		@include border-top-right-radius(0);
		@include border-bottom-right-radius(0);
	}

}
#menu2 {
	position: relative;
	top: -4em;
	left: 39.6em;
		a.dropdown {		
		@include border-top-left-radius(0);
		@include border-bottom-left-radius(0);
		@include border-top-right-radius(0);
		@include border-bottom-right-radius(0);
	}
}

#menu3 {
	position: relative;
	top: -4em;
	left: 39.3em;
	a.dropdown {		
		@include border-top-left-radius(0);
		@include border-bottom-left-radius(0);
	}
}

#articlemenu1 {
	position: relative;
	top: 2.7em;
	left: 52.6em;
	font-size: 0.8em;
	a.dropdown {		
		@include border-top-right-radius(0);
		@include border-bottom-right-radius(0);
	}
	float:left;
}

#articlemenu2 {
	position: relative;
	top: 2.7em;
	left: 52.5em;
	font-size: 0.8em;
	a.dropdown {		
		@include border-top-left-radius(0);
		@include border-bottom-left-radius(0);
	}
	float:left;
}

.dropdown-menu {
	@include single-text-shadow(black, 0, 0px, 1px);
	color: $darkgray;
	width: 400px;
	img {
		width: 100px;
		height: 100px;
		float: right;
	}

	dl {
		width: 260px;
	}

	dt {
		font-size: 1.4em;
	}

	dd {
		font-size: 1.8em;
		padding-left: 2em;
	}
}


a.dropdown:hover { 
	@include linear-gradient(color-stops(lighten($darkblue,32%), lighten($darkblue,29%), lighten($darkblue, 26%)));
	color: $lightgray;
}


a.dropdown { position: relative; margin-left: 3px; }

//a.dropdown span {  background-repeat: no-repeat; background-position: 100% 50%; padding: 4px 16px 6px 0; }
a.dropdown.dropdown-active { color:$darkgray; background-color:#ddeef6; }
.dropdown-menu  { 
	
	padding:0;
	position:absolute; 
	top:14px; 
	right:-2px; 
	display:none; 
	z-index:5000; 
	@include border-radius(30px, 30px); 
	@include border-top-right-radius(0);
	border: rgba(30,30,30,0.7) 10px solid;

	
}

.inner {
	h3 {
		text-align: right;
	}
	padding:7px 12px; 
	@include linear-gradient(color-stops(lighten($translucentdarkblue,26%), lighten($translucentdarkblue,29%)));
	@include border-radius(19px, 19px); 
	@include border-top-right-radius(0);

	margin:0;
	dd {
		a:link {
		@include single-text-shadow($darkblue, 0, 0px, 1px);

		}
	}
}


  //.dropdown-menu p { font-size:1px; }
.dropdown-menu a:link, .dropdown-menu a:visited  {

		@include single-text-shadow($darkblue, 0, 0px, 1px);
	display: inline;
 font-weight:bold; color:$lightgray; text-decoration:none; ;

 }
.dropdown-menu a:active, .dropdown-menu a:hover { color:$darkgray; }

      * html .dropdown-menu { top:28px; }
      *+ html .dropdown-menu { top:28px; }

/* dropdowns: specific */
#menu1      { float:left; margin-right:20px; }
  #dropdown1  { width:340px; }
#menu2      { float:left; }
  #dropdown2  { width:340px;  }
#menu3		{ float: left;}
	#dropdown3 { width:340px; }
.relative    { position:relative; }

.str{color:#b58900}
.kwd{color:#cb4b16}
.com{color:#dc322f}
.typ{color:#d33682}
.lit{color:#6c71c4}
.pun{color:#268bd2}
.pln{color:#2aa198}
.tag{color:#859900}
.atn{color:#d33682}
.atv{color:#6c71c4}
.dec{color:#268bd2}


